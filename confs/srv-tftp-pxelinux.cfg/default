    MENU DEFAULT
    MENU LABEL vmlinuz-4.9.0-8-amd64 + zmodyfikownany initramfs
    KERNEL kernel/vmlinuz-4.9.0-8-amd64
    APPEND initrd=kernel/initramfs.igz

LABEL linux-3
